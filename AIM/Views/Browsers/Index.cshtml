@model IEnumerable<AIM.Models.Browser>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BrowserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BrowserCreatedDt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BrowserUpdatedDt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
         
     
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BrowserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BrowserCreatedDt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BrowserUpdatedDt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status.StatusName)
            </td>
            
          
            <td>
                <a asp-action="Edit" asp-route-id="@item.BrowserCode">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BrowserCode">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BrowserCode">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
