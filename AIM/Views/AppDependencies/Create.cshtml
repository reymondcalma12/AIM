@model AIM.Models.AppDependency

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>AppDependency</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="RPortOslaCode" class="control-label"></label>
                <select asp-for="RPortOslaCode" class ="form-control" asp-items="ViewBag.RPortOslaCode"></select>
            </div>
            <div class="form-group">
                <label asp-for="AppCode" class="control-label"></label>
                <select asp-for="AppCode" class ="form-control" asp-items="ViewBag.AppCode"></select>
            </div>
            <div class="form-group" hidden>
                <label asp-for="DependencyCreatedBy" class="control-label"></label>
                <select asp-for="DependencyCreatedBy" class ="form-control" asp-items="ViewBag.DependencyCreatedBy"></select>
            </div>
            <div class="form-group" hidden>
                <label asp-for="DependencyCreatedDt" class="control-label"></label>
                <input asp-for="DependencyCreatedDt" class="form-control" value="@DateTime.Now.ToString("yyyy-MM-ddTHH:mm:ss")" />
                <span asp-validation-for="DependencyCreatedDt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
