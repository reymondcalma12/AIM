@model IEnumerable<AIM.Models.Level3>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               Name
            </th>
       
            <th>
                Status
            </th>
            <th>Created By</th>
            <th>Date Created</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
        
            <td>
                @Html.DisplayFor(modelItem => item.Level3Name)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.Status.StatusName)
            </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Level3CreatedBy)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Level3CreatedDt)
                </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Level3Code">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Level3Code">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Level3Code">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
