@model AIM.Models.AppContact

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>AppContact</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AppCode" />
            <input type="hidden" asp-for="ContactNo" />
            <input type="hidden" asp-for="SupportTypeCode" />
            <div class="form-group" hidden>
                <label asp-for="CreatedBy" class="control-label"></label>
                <select asp-for="CreatedBy" class="form-control" asp-items="ViewBag.CreatedBy"></select>
                <span asp-validation-for="CreatedBy" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="CreatedDt" class="control-label"></label>
                <input asp-for="CreatedDt" class="form-control" />
                <span asp-validation-for="CreatedDt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
