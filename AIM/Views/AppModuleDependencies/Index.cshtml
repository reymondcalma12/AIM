@model IEnumerable<AIM.Models.AppModuleDependency>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            
         
            <th>
                Dependency Application
            </th>
            <th>
                Created By
            </th>
            <th>
                Created Date
            </th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
         
            <td>
                @Html.DisplayFor(modelItem => item.DependencyApplication.AppName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.UserFullName)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedDt)
                </td>
            <td>
                    
                    @Html.ActionLink("Delete", "Delete", new { id = $"{item.AppCode}-{item.ModuleCode}" })
            </td>
        </tr>
}
    </tbody>
</table>
