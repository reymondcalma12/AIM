@model IEnumerable<AIM.Models.Module>

@{
    ViewData["Title"] = "Index"; Layout = null;
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            

             <th>
                 Category
             </th>

            <th>
Title
            </th>
 <th>Status</th>     
 <th>Created By</th>
 <th>Date Created</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>

          
                <td>
                    @Html.DisplayFor(modelItem => item.Category.CategoryName)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleTitle)
            </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Status.StatusName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ModuleCreated)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ModuleDtCreated)


                </td>
            
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.ModuleId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ModuleId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ModuleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
