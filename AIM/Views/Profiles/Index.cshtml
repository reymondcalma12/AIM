@model IEnumerable<AIM.Models.Profile>

@{
    ViewData["Title"] = "Index"; Layout = null;
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
          
            <th>
                Name
            </th>
            <th>
Description            
            </th>
            <th>Created By</th>
            <th>Date Created</th>
            
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
              
            <td>
                @Html.DisplayFor(modelItem => item.ProfileName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfileDescription)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProfileCreated)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProfileDtCreated)
                </td>
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProfileId">Edit</a>
                <a asp-action="Details" asp-route-id="@item.ProfileId">Details</a>
                <a asp-action="Delete" asp-route-id="@item.ProfileId">Delete</a>
                    <a asp-controller="ProfileAccesses" asp-action="Index" asp-route-id="@item.ProfileId"><i class="fas fa-user-cog"></i> Modify</a>
            </td>
        </tr>
}
    </tbody>
</table>

